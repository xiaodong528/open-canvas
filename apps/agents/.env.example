# =============================================================================
# Open Canvas - TypeScript LangGraph Backend Environment Variables
# =============================================================================
# Copy this file to .env and fill in your values
# Related examples:
# - Python LangGraph backend: apps/agents-py/.env.example
# - Next.js frontend: apps/web/.env.example
# =============================================================================

# This file should contain secrets used inside the agent (apps/agents)

# LangSmith tracing
LANGSMITH_TRACING="true"
LANGSMITH_API_KEY=""
# Optional, unless using a non default project
# LANGCHAIN_PROJECT=""
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
# ---LLM API Keys---

# Anthropic
ANTHROPIC_API_KEY=""
# OpenAI
OPENAI_API_KEY=""
# Fireworks
FIREWORKS_API_KEY=""
# Gemini
GOOGLE_API_KEY=""
# Groq - STT
GROQ_API_KEY=""

# -----------------------------------------------------------------------------
# Azure OpenAI (Optional - only if using Azure)
# IMPORTANT: These variables MUST be prefixed with an underscore
# -----------------------------------------------------------------------------
# _AZURE_OPENAI_API_KEY=""
# _AZURE_OPENAI_API_INSTANCE_NAME=""
# _AZURE_OPENAI_API_DEPLOYMENT_NAME=""
# _AZURE_OPENAI_API_VERSION="2024-08-01-preview"
# _AZURE_OPENAI_API_BASE_PATH=""

# -----------------------------------------------------------------------------
# Ollama (Optional - for local LLM)
# Default: http://host.docker.internal:11434
# -----------------------------------------------------------------------------
# OLLAMA_API_URL="http://host.docker.internal:11434"

# -----------------------------------------------------------------------------
# External Services
# -----------------------------------------------------------------------------
# Exa Search API (for web_search graph)
EXA_API_KEY=""
